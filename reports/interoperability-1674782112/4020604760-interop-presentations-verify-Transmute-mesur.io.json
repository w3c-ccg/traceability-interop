      {
        "id": "0dfc2c27-9040-47e7-a066-a6e49564c564",
        "name": "Get Organization DIDs (Issuer)",
        "request": {
          "url": {
            "path": [
              "identifiers",
              "{{ISSUER_ORGANIZATION_DID_WEB}}"
